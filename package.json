    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@hapi/hawk": "^7.1.0",
    "@loadable/component": "^5.12.0",
    "autoprefixer": "^8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
